execute at @s[tag=EtiFeld.p3DimensionenFelderAnheben] run tellraw @a[distance=..25] ["Direktzugriffsspeicher.plus.3:\n",{"text":"Feld[[","bold":true},{"score":{"name":"#VarFeld.p3AktuellePosition","objective":"PZFeld.p3Pos"},"bold":true,"color":"dark_green"},{"text":"]].Länge + 1 = ","bold":true},{"score":{"name":"#VarFeld.p3WertInhalt","objective":"PZFeld.p3Wert"},"bold":true,"color":"red"} ]

